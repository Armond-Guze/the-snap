{
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/.next/**": true,
    "**/.turbo/**": true,
    "**/dist/**": true,
    "**/out/**": true,
    "**/.vercel/**": true,
    "**/sanity/.cache/**": true,
    "**/sanity/node_modules/**": true
  },
  "search.exclude": {
    "**/node_modules/**": true,
    "**/.next/**": true,
    "**/.turbo/**": true,
    "**/dist/**": true,
    "**/out/**": true,
    "**/.vercel/**": true,
    "**/sanity/.cache/**": true
  },
  "typescript.tsserver.maxTsServerMemory": 4096,
  "typescript.disableAutomaticTypeAcquisition": true
}
